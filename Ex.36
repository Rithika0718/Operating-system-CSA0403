#include <stdio.h>

int main() {
    int index[10], n, i;
    printf("Enter number of blocks: ");
    scanf("%d", &n);

    printf("Enter block numbers:\n");
    for (i = 0; i < n; i++) scanf("%d", &index[i]);

    printf("Index block contains:\n");
    for (i = 0; i < n; i++)
        printf("Block %d\n", index[i]);
    return 0;
}
