#include <stdio.h>

int main() {
    int pages[20], frames[10], n, f, i, j, k = 0, fault = 0;
    printf("Enter pages: ");
    scanf("%d", &n);
    for (i = 0; i < n; i++) scanf("%d", &pages[i]);

    printf("Enter frames: ");
    scanf("%d", &f);
    for (i = 0; i < f; i++) frames[i] = -1;

    for (i = 0; i < n; i++) {
        int hit = 0;
        for (j = 0; j < f; j++)
            if (frames[j] == pages[i]) hit = 1;

        if (!hit) {
            frames[k] = pages[i];
            k = (k + 1) % f;
            fault++;
        }
    }
    printf("Page Faults: %d\n", fault);
    return 0;
}
